<div class="row">
  <div class="span12">
    <legend>Stock information for <%= @stock.name %></legend>
    <table class="table table-striped table-bordered responsive-table">
      <thead>
        <tr>
          <th>Ticker</th>
          <th>Name</th>
          <th>Change +/-</th>
          <th>Last Trade</th>
          <th>Time</th>
          <th>Open</th>
          <th>Highest</th>
          <th>Lowest</th>
          <th>Volume</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td data-title="Ticker"><%= @stock.ticker %></td>
          <td data-title="Name"><%= @stock.name %></td>
          <td data-title="Change"><%= @stockdata[0] %></td>
          <td data-title="Last Trade"><%= @stockdata[1] %></td>
          <td data-title="Time"><%= @stockdata[2] %></td>
          <td data-title="Open"><%= @stockdata[3] %></td>
          <td data-title="Highest"><%= @stockdata[4] %></td>
          <td data-title="Lowest"><%= @stockdata[5] %></td>
          <td data-title="Volume"><%= @stockdata[6] %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>